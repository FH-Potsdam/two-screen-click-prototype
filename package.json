{
  "name": "two-screen-click-prototype",
  "version": "1.0.0",
  "description": "Electron App to click through a two-screen slideshow.",
  "main": "main.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "dev": "electron ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FH-Potsdam/two-screen-click-prototype.git"
  },
  "author": "sebastian-meier",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FH-Potsdam/two-screen-click-prototype/issues"
  },
  "homepage": "https://github.com/FH-Potsdam/two-screen-click-prototype#readme",
  "build": {
    "npmRebuild": false,
    "nodeGypRebuild": false,
    "files": [
      "**/*",
      "!data/**/*",
      "!**/node_modules/*/{CHANGELOG.md,README.md,README,readme.md,readme}",
      "!**/node_modules/*/{test,__tests__,tests,powered-test,example,examples}",
      "!**/node_modules/*.d.ts",
      "!**/node_modules/.bin",
      "!**/*.{iml,o,hprof,orig,pyc,pyo,rbc,swp,csproj,sln,xproj}",
      "!.editorconfig",
      "!**/._*",
      "!**/{.DS_Store,.git,.hg,.svn,CVS,RCS,SCCS,.gitignore,.gitattributes}",
      "!**/{__pycache__,thumbs.db,.flowconfig,.idea,.vs,.nyc_output}",
      "!**/{appveyor.yml,.travis.yml,circle.yml}",
      "!**/{npm-debug.log,yarn.lock,.yarn-integrity,.yarn-metadata.json}"
    ]
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.2.1",
    "@electron-forge/maker-deb": "^6.2.1",
    "@electron-forge/maker-rpm": "^6.2.1",
    "@electron-forge/maker-squirrel": "^6.2.1",
    "@electron-forge/maker-zip": "^6.2.1",
    "electron": "^25.2.0",
    "electron-squirrel-startup": "^1.0.0"
  }
}
